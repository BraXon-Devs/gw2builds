{
  "name": "gw2builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "xo",
    "test": "mocha --compilers js:babel-core/register --require tests/tools/requireCSS.js tests/**/*.spec.js{,x}",
    "build": "gulp build",
    "production": "NODE_ENV=production gulp build",
    "dev": "gulp dev",
    "start": "node bin/www"
  },
  "description": "Guild Wars 2 build editor",
  "main": "app.js",
  "author": "Archomeda",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-intl": "^2.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-root-import": "^4.1.5",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.6",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "change-case": "^3.0.0",
    "classnames": "^2.2.5",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "enzyme": "^2.7.1",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rev-all": "^0.9.7",
    "gulp-sourcemaps": "^2.3.1",
    "gulp-to-ico": "^1.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "gw2api-client": "^5.0.0",
    "gw2api-extension": "file:lib/gw2api-extension",
    "gw2be-api-extension-data": "file:lib/gw2be-api-extension-data",
    "gw2be-build-string": "file:lib/gw2be-build-string",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "modular-css": "^2.0.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.2",
    "react-onclickoutside": "^5.8.3",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.8.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 4,
    "extends": "xo-react",
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^[~]"
          ]
        }
      ],
      "linebreak-style": "off",
      "no-div-regex": "off",
      "no-negated-condition": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "react/jsx-closing-bracket-location": [
        "error",
        "after-props"
      ],
      "react/no-unused-prop-types": "warn",
      "unicorn/filename-case": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
