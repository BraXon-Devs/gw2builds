{
  "name": "gw2builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "xo",
    "test-lib": "mocha lib/**/*.spec.js --compilers js:babel-core/register",
    "build": "gulp build",
    "production": "NODE_ENV=production gulp build",
    "dev": "gulp dev",
    "start": "node bin/www"
  },
  "description": "Guild Wars 2 build editor",
  "main": "app.js",
  "author": "Archomeda",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "autoprefixer": "^6.5.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.5",
    "browserify": "^13.1.0",
    "btoa": "^1.1.2",
    "chai": "^3.5.0",
    "change-case": "^3.0.0",
    "cssnano": "^3.8.0",
    "del": "^2.2.2",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-nodemon": "^2.2.1",
    "gulp-rev-all": "^0.9.7",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-to-ico": "^1.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gw2api-client": "^4.0.1",
    "jbinary": "^2.1.3",
    "lodash": "^4.16.4",
    "mocha": "^3.2.0",
    "modular-css": "^0.28.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.5",
    "react-onclickoutside": "^5.7.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "xo": "^0.17.0"
  },
  "xo": {
    "space": 4,
    "extends": "xo-react",
    "rules": {
      "linebreak-style": "off",
      "no-negated-condition": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "react/jsx-closing-bracket-location": [
        "error",
        "after-props"
      ],
      "react/no-unused-prop-types": "warn",
      "unicorn/filename-case": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  }
}
