{
  "name": "gw2builds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "xo",
    "test": "npm run test:coverage --silent",
    "test:mocha": "mocha --compilers js:babel-core/register --require tests/tools/enzyme.js --require tests/tools/requireCSS.js \"tests/**/*.spec.js{,x}\"",
    "test:coverage": "nyc --all --include=src --extension .jsx --reporter=lcov --reporter=text-summary --report-dir=coverage npm run test:mocha",
    "build": "gulp build",
    "production": "NODE_ENV=production gulp build",
    "dev": "gulp dev",
    "start": "node server/bin/www"
  },
  "description": "Guild Wars 2 build editor",
  "main": "app.js",
  "author": "Archomeda",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "modular-css-short-namer": "^0.0.1",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "redux-devtools-extension": "^2.13.7",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browser-pack-flat": "^3.2.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "change-case": "^3.0.2",
    "classnames": "^2.2.6",
    "common-shakeify": "^0.5.2",
    "cssnano": "^4.1.7",
    "del": "^3.0.0",
    "envify": "^4.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-nodemon": "^2.4.2",
    "gulp-rev-all": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-to-ico": "^1.0.0",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "gw2-tooltip-html": "^0.0.3",
    "gw2api-client": "6.0.5",
    "gw2api-extension": "file:./lib/gw2api-extension",
    "gw2be-api-extension-data": "file:./lib/gw2be-api-extension-data",
    "gw2be-build-string": "file:./lib/gw2be-build-string",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "modular-cssify": "^8.1.0",
    "nyc": "^13.1.0",
    "postcss-import": "^12.0.1",
    "postcss-urlrewrite": "^0.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-promise": "^0.6.0",
    "redux-sync-reducer": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "run-sequence": "^2.2.1",
    "serviceworker-storage": "^0.1.0",
    "through2": "^3.0.0",
    "typeface-open-sans": "^0.0.54",
    "uglifyify": "^5.0.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": 4,
    "extends": "xo-react",
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "^[~]"
          ]
        }
      ],
      "linebreak-style": "off",
      "no-div-regex": "off",
      "no-negated-condition": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "react/jsx-closing-bracket-location": [
        "error",
        "after-props"
      ],
      "react/no-unused-prop-types": "warn",
      "unicorn/filename-case": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
